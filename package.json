    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.13.0",
    "body-parser": "^1.15.2",
    "colors": "^1.1.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "express-validator": "^2.20.8",
    "jwt-simple": "^0.5.0",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
